<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.layout.splash.SplashActivity">

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottieSplashKwang0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        app:lottie_autoPlay="true"
        app:lottie_loop="false"
        app:lottie_rawRes="@raw/splash_success"
        app:lottie_speed="2" />

    <com.kwang0.introduce.ui.view.kwang0.Kwang0TextView
        android:id="@+id/textSplashKwang0Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:padding="@dimen/dimen.size64" />

</FrameLayout>
